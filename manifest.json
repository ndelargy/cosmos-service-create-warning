{
  "name": "Cosmos Service Create Warning",
  "version": "1.0",
  "description": "Warn me when I'm about to create a service when I should create a lambda!",
  "content_scripts": [
    {
      "matches": ["*://cosmos.tools.bbc.co.uk/services/create*"],
      "js": ["warn.js"],
      "css": ["style.css"]
    }
  ],
  "page_action": {
    "default_icon": {
      "16": "images/no-cosmos-create-16.png",
      "32": "images/no-cosmos-create-32.png",
      "48": "images/no-cosmos-create-48.png",
      "128": "images/no-cosmos-create-128.png"
    }
  },
  "icons": {
    "16": "images/no-cosmos-create-16.png",
    "32": "images/no-cosmos-create-32.png",
    "48": "images/no-cosmos-create-48.png",
    "128": "images/no-cosmos-create-128.png"
  },
  "manifest_version": 2
}
